{
  "buildCommand": null,
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/news",
      "destination": "/news.html"
    },
    {
      "source": "/blog",
      "destination": "/blog.html"
    },
    {
      "source": "/career",
      "destination": "/career.html"
    },
    {
      "source": "/blog-detail-1",
      "destination": "/blog-detail-1.html"
    },
    {
      "source": "/blog-detail-2",
      "destination": "/blog-detail-2.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}

